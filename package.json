{
  "name": "pimath",
  "version": "0.2.2",
  "description": "A math library for teacher :)",
  "type": "module",
  "main": "dist/pimath.js",
  "module": "dist/pimath.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/pimath.js",
      "require": "./dist/pimath.js"
    }
  },
  "./types/index.d.ts": "./types/index.d.ts",
  "files": [
    "dist",
    "types",
    "src"
  ],
  "scripts": {
    "dev": "vite serve",
    "test": "vitest",
    "build": "tsc --p ./tsconfig-build.json && vite build",
    "preview": "vite preview",
    "docs": "typedoc",
    "dependency-cruise": "dependency-cruise -T dot -x node_modules -v -- src/index.ts  | dot -T svg > dependencies.svg"
  },
  "keywords": [
    "math",
    "algebra"
  ],
  "author": "Basil Gass",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basilgass/PiMath.git"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@vitejs/plugin-vue": "^6.0.4",
    "autoprefixer": "^10.4.24",
    "dependency-cruiser": "^17.3.8",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "tailwindcss": "^4.1.18",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.18",
    "vue-router": "^5.0.2"
  },
  "dependencies": {
    "piexpression": "^0.1.3"
  }
}
