{
  "name": "pimath",
  "version": "0.1.7",
  "description": "A math library for teacher :)",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/pimath.js",
      "require": "./dist/pimath.js"
    },
    "./types": {
      "import": "./types/index.d.ts",
      "require": "./types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "test": "vitest",
    "dev": "vite serve",
    "build": "tsc --p ./tsconfig-build.json && vite build",
    "preview": "vite preview",
    "docs": "typedoc",
    "dependency-cruise": "dependency-cruise -T dot -x node_modules -v -- src/index.ts  | dot -T svg > dependencies.svg"
  },
  "keywords": [
    "math",
    "algebra"
  ],
  "author": "Basil Gass",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@types/eslint__js": "^8.42.3",
    "autoprefixer": "^10.4.19",
    "dependency-cruiser": "^16.3.10",
    "eslint": "^8.57.0",
    "globals": "^15.3.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0",
    "vite": "^5.2.12",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "piexpression": "^0.0.8"
  }
}
