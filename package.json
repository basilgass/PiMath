{
  "name": "pimath",
  "version": "0.1.32",
  "description": "A math library for teacher :)",
  "type": "module",
  "main": "dist/pimath.js",
  "module": "dist/pimath.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/pimath.js",
      "require": "./dist/pimath.js"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "vite serve",
    "test": "vitest",
    "build": "tsc --p ./tsconfig-build.json && vite build",
    "preview": "vite preview",
    "docs": "typedoc",
    "dependency-cruise": "dependency-cruise -T dot -x node_modules -v -- src/index.ts  | dot -T svg > dependencies.svg"
  },
  "keywords": [
    "math",
    "algebra"
  ],
  "author": "Basil Gass",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basilgass/PiMath.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/eslint__js": "^8.42.3",
    "autoprefixer": "^10.4.21",
    "dependency-cruiser": "^16.10.1",
    "eslint": "^9.25.1",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.2",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "piexpression": "^0.1.0"
  }
}
