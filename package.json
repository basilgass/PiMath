{
  "name": "pimath",
  "version": "0.1.18",
  "description": "A math library for teacher :)",
  "type": "module",
  "main": "dist/pimath.js",
  "module": "dist/pimath.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/pimath.js",
      "require": "./dist/pimath.js",
      "types": "./types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "dev": "vite serve",
    "build": "tsc --p ./tsconfig-build.json && vite build",
    "preview": "vite preview",
    "docs": "typedoc",
    "dependency-cruise": "dependency-cruise -T dot -x node_modules -v -- src/index.ts  | dot -T svg > dependencies.svg"
  },
  "keywords": [
    "math",
    "algebra"
  ],
  "author": "Basil Gass",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.11.0",
    "@types/eslint__js": "^8.42.3",
    "autoprefixer": "^10.4.20",
    "dependency-cruiser": "^16.3.10",
    "eslint": "^9.11.0",
    "globals": "^15.9.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.12",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.7",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0",
    "vite": "^5.4.7",
    "vite-plugin-dts": "^4.2.1",
    "vitest": "^2.1.1"
  },
  "dependencies": {
    "piexpression": "^0.0.8"
  }
}
