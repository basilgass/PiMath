{
  "name": "pimath",
  "version": "0.1.33",
  "description": "A math library for teacher :)",
  "type": "module",
  "main": "dist/pimath.js",
  "module": "dist/pimath.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/pimath.js",
      "require": "./dist/pimath.js"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "vite serve",
    "test": "vitest",
    "build": "tsc --p ./tsconfig-build.json && vite build",
    "preview": "vite preview",
    "docs": "typedoc",
    "dependency-cruise": "dependency-cruise -T dot -x node_modules -v -- src/index.ts  | dot -T svg > dependencies.svg"
  },
  "keywords": [
    "math",
    "algebra"
  ],
  "author": "Basil Gass",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basilgass/PiMath.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/eslint__js": "^9.14.0",
    "autoprefixer": "^10.4.21",
    "dependency-cruiser": "^17.0.1",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.12",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vite": "^7.1.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "piexpression": "^0.1.0"
  }
}
